<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8.1" />
    </startup>

	<system.serviceModel>
		<services>
			<service name="CalculatorService.CalculatorService" behaviorConfiguration="mexBehaviour">
				<endpoint address="CalculatorService" binding="basicHttpBinding" contract="CalculatorService.ICalculatorService"></endpoint>
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8085/"/>
					</baseAddresses>
				</host>
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="mexBehaviour">
					<serviceMetadata httpGetEnabled="true"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
	
</configuration>